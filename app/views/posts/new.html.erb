<h1>New Post</h1>
<%= form_with :model => @post do |form| %>
  <p>
    <%= form.label "Author Name" %>
    <%= form.text_field "author" %>
  </p>
  <p>
    <%= form.label "body" %>
    <%= form.text_field "body" %>
    <%= form.label "image" %>
    <%= form.text_field "image" %>
  </p>
  <%= form.submit %>
<% end %>

<p>
<a href="/posts/"> Go back to tacos main page </a>
</p>